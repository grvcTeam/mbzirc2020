  <!-- spawn gazebo model -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find atrvjr_description)/urdf/atrvjr_ur5.urdf.xacro'" />
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -param robot_description -model robot"/>

  <!-- controllers -->
  <include file="$(find ur_gazebo)/launch/controller_utils.launch"/>

  <rosparam file="$(find atrvjr_description)/config/arm_controller_ur5.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="spawner" args="arm_controller" output="screen"/>

  <rosparam file="$(find atrvjr_description)/config/base_controller.yaml" command="load"/>
  <node name="base_controller_spawner" pkg="controller_manager" type="spawner" output="screen" args="mobile_base_controller"/>

</launch>
