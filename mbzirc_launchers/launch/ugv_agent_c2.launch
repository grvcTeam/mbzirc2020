<?xml version="1.0"?>
<launch>

  <arg name="model" default="$(find ur_e_description)/urdf/ur5e_robot.urdf.xacro" />
  <param name="robot_description" command="$(find xacro)/xacro --inorder $(arg model)" />   

  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model mbzirc2020_0 -z 0.1" respawn="false" output="screen" />

  <include file="$(find ur_e_gazebo)/launch/controller_utils.launch"/>
  <rosparam file="$(find ur_e_gazebo)/controller/arm_controller_ur5e.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller base_controller" respawn="false" output="screen"/>


  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" ns="UGV" output="screen" clear_params="true">
        <rosparam command="load" file="$(find robot_localization)/params/ekf_template.yaml" /> <!-- CHANGE TOPICS FOR REAL ROBOT -->
  </node>

  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform_node" clear_params="true">
        <rosparam command="load" file="$(find robot_localization)/params/navsat_transform_template.yaml" />
        <remap from="imu/data" to="/imu_plugin"/>
        <remap from="gps/fix" to="/fix"/>
        <remap from="odometry/filtered" to="odometry/filtered"/>   <!-- CHANGE TOPICS FOR REAL ROBOT -->
  </node>


  <include file = "$(find husky_navigation)/launch/move_base_mapless_demo.launch"/>
  <include file = "$(find mbot_pregrasp_planning)/ros/launch/pregrasp_planner.launch"/>
  <include file = "$(find ur5_e_moveit_config_joao)/launch/move_group.launch"/>
  <!-- <include file = "$(find pcl_ros)/samples/pcl_ros/segmentation/mbzirc_pose_estimation.launch"/> -->
  <!-- <include file = "$(find ur5_magnetic_gripper)/launch/magnetic_gripper.launch"/> -->
  <include file = "$(find laser_line_extraction)/launch/example.launch"/>


  <node pkg="robot_action_servers" type="ugv_action_server" name="server" output="screen"/>

</launch>
