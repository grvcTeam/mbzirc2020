<?xml version="1.0"?>
<launch>

  <group ns="mbzirc2020_0"> 
    
    <arg name="model" default="$(find ur_e_description)/urdf/ur5e_robot.urdf.xacro" />
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(arg model)" />   

    <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model mbzirc2020_0 -z 0.1" respawn="false" output="screen" />

    <rosparam file="$(find ur_e_gazebo)/controller/arm_controller_ur5e.yaml" command="load"/>

    <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller" respawn="false" output="screen"/>
    <node name="base_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn base_controller" respawn="false" output="screen"/>

    <include file="$(find ur_e_gazebo)/launch/controller_utils.launch"/>

    <include file="$(find robot_localization)/launch/ekf_template.launch" />

    <include file="$(find robot_localization)/launch/navsat_transform_template.launch" />

    <include file="$(find atrvjr)/launch/bringup_common.launch" /> -->

    <node pkg="robot_action_servers" type="ugv_action_server" name="server" output="screen"/>

  </group>

</launch>
