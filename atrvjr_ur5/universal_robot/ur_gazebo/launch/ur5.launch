<?xml version="1.0"?>
<launch>
  <arg name="limited" default="false"  doc="If true, limits joint range [-PI, PI] on all joints." />
  <arg name="paused" default="false" doc="Starts gazebo in paused mode" />
  <arg name="gui" default="false" doc="Starts gazebo gui" />
  
  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" default="$(find challenges_worlds)/worlds/challenge2.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- send robot urdf to param server -->
  <include file="$(find ur_description)/launch/ur5_upload.launch">
    <arg name="limited" value="$(arg limited)"/>
  </include>


  <include file="$(find ur_gazebo)/launch/controller_utils.launch"/>

<!--   <rosparam file="$(find ur_gazebo)/controller/gazebo_control_fix.yaml" command="load"/>
 -->
  <rosparam file="$(find ur_gazebo)/controller/arm_controller_ur5.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="spawner" output="screen" args="arm_controller"/>

  <rosparam file="$(find ur_gazebo)/controller/base_controller.yaml" command="load"/>
  <node name="base_controller_spawner" pkg="controller_manager" type="spawner" output="screen" args="mobile_base_controller"/>


    <!-- push robot_description to factory and spawn robot in gazebo -->

  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -param robot_description -model robot 
    -J ur5_shoulder_pan_joint 0.0
    -J ur5_shoulder_lift_joint 0.0
    -J ur5_elbow_joint 0.0
    -J ur5_wrist_1_joint 0.0
    -J ur5_wrist_2_joint 0.0
    -J ur5_wrist_3_joint 0.0
    -J teste_joint 0.0
    -x -3 -z 0.1"/>



  <!-- <rosparam file="$(find ur_gazebo)/controller/arm_controller_ur5_position.yaml" command="load"/>
  <node pkg="controller_manager" type="spawner" name="pos_arm_ctrl_spawner"
          output="screen" args="
                              ur5_shoulder_pan_joint_position_controller
                              ur5_shoulder_lift_joint_position_controller
                              ur5_elbow_joint_position_controller
                              ur5_wrist_1_joint_position_controller
                              ur5_wrist_2_joint_position_controller
                              ur5_wrist_3_joint_position_controller" />

 -->
 <!--  <rosparam file="$(find ur_gazebo)/controller/arm_controller_ur5_position.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn ur5_shoulder_pan_joint_position_controller
  ur5_shoulder_lift_joint_position_controller
  ur5_elbow_joint_position_controller
  ur5_wrist_1_joint_position_controller
  ur5_wrist_2_joint_position_controller
  ur5_wrist_3_joint_position_controller" respawn="false" output="screen"/>
 -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ur_gazebo)/rviz_atrv.rviz" />

</launch>
