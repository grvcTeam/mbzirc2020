<launch>

   <!-- base_link -> camera_link transform -->
   <node pkg="tf" type="static_transform_publisher" name="base2camera_link_tf_publisher" args="0.16 0 -0.06 0 0.4297622790966884 0 /base_link /camera_link 100"/>

   <!-- dji tf publisher -->
   <node pkg="drone_detector_tracking" type="dji_tf.py" name="dji_tf_publisher" output="screen"/>

   <!-- drone detection -->
   <!-- <node pkg="drone_detector_tracking" type="drone_detector_tracking_node" name="drone_detector" output="screen">
      <param name="rgb_img_topic" type="str" value="/dji_f550_camera_2/rgb/image_raw/compressed"/>
      <param name="depth_img_topic" type="str" value="/dji_f550_camera_2/depth/image_raw"/>
      <param name="camera_info_topic" type="str" value="/dji_f550_camera_2/rgb/camera_info"/>
      <param name="uav_pose_topic" type="str" value="/dji_f550_2/ual/pose"/>
      <param name="publish_debug_images" type="bool" value="true"/>
   </node>
 -->
   <node pkg="drone_detector_tracking" type="drone_detector_tracking_node" name="drone_detector" output="screen">
      <param name="rgb_img_topic" type="str" value="/camera/color/image_rect_color/compressed"/>
      <param name="depth_img_topic" type="str" value="/camera/aligned_depth_to_color/image_raw"/>
      <param name="camera_info_topic" type="str" value="/camera/color/camera_info"/>
      <param name="uav_pose_topic" type="str" value="/dji_control/pose"/>
      <param name="publish_debug_images" type="bool" value="true"/>
   </node>


</launch>
